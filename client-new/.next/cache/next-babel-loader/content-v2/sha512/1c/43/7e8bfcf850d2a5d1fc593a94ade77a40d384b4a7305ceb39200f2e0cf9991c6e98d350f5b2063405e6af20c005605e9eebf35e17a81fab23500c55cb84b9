';\r\nimport Products from \"../grid/productGrid.style\";\r\nimport StarRating from \"@/components/Product/StarRating\";\r\nimport {useDispatch, useSelector} from \"react-redux\";\r\nimport {getItemCartQty} from \"@/utils/cartUtils\";\r\nimport {addToCart} from \"@/redux/actions/cartActions\";\r\nimport {ArrowNext} from \"@/assets/icons/ArrowNext\";\r\ntype ProductDetailsProps = {\r\n  product: any;\r\n  deviceType: {\r\n    mobile: boolean;\r\n    tablet: boolean;\r\n    desktop: boolean;\r\n  };\r\n};\r\n\r\nconst ProductDetails: React.FunctionComponent<ProductDetailsProps> = ({\r\n                                                                        product,\r\n                                                                        deviceType,\r\n                                                                      }) => {\r\n  const cartState = useSelector((state:any) => state.cartReducer);\r\n  const {currency:{symbol}} = useSelector((state:any) => state.shopReducer);\r\n  const cartQuantity=getItemCartQty(cartState,product.id)\r\n  const dispatch=useDispatch()\r\n\r\n\r\n  const handleAddClick = (e: { stopPropagation: () => void; }) => {\r\n    e.stopPropagation();\r\n    dispatch(addToCart(product,1))\r\n\r\n  };\r\n  const handleRemoveClick = (e: { stopPropagation: () => void; }) => {\r\n    e.stopPropagation();\r\n    dispatch(addToCart(product,-1))\r\n\r\n  };\r\n\r\n  useEffect(() => {\r\n    setTimeout(() => {\r\n      window.scrollTo(0, 0);\r\n    }, 500);\r\n  }, []);\r\n\r\n  return (\r\n      <>\r\n        <ProductDetailsWrapper className=\"product-card\" dir=\"ltr\">\r\n          <ProductPreview>\r\n            <BackButton>\r\n              <Button\r\n                  type=\"button\"\r\n                  size=\"small\"\r\n                  style={{\r\n                    backgroundColor: '#ffffff',\r\n                    border: '1px solid #f1f1f1',\r\n                    color: '#77798c',\r\n                  }}\r\n                  onClick={Router.back}\r\n              >\r\n                <LongArrowLeft style={{ marginRight: 5 }} />\r\n                Back              </Button>\r\n            </BackButton>\r\n            {product.thumbImage?<CarouselWithCustomDots\r\n                items={[product.thumbImage,...product.images]}\r\n                deviceType={deviceType}\r\n            />:<CarouselWithCustomDots\r\n                items={['https://res.cloudinary.com/wisecart/image/upload/v1622387938/nwxgnej1x6yvugrb8lzv.png']}\r\n                deviceType={deviceType}\r\n            />}\r\n\r\n          </ProductPreview>\r\n\r\n          <ProductInfo dir={'ltr'}>\r\n            <ProductTitlePriceWrapper>\r\n              <ProductTitle>{product.name}</ProductTitle>\r\n              <ProductPriceWrapper>\r\n                {product.discountInPercent ? (\r\n                    <SalePrice>\r\n                      {symbol}\r\n                      {product.price}\r\n                    </SalePrice>\r\n                ) : null}\r\n\r\n                <ProductPrice>\r\n                  {symbol}\r\n                  {product.salePrice ? product.salePrice : product.price}\r\n                </ProductPrice>\r\n              </ProductPriceWrapper>\r\n            </ProductTitlePriceWrapper>\r\n            <StarRating rating={4}/>\r\n            {/*<ProductWeight>{product.unit}</ProductWeight>*/}\r\n            {/*<ProductDescription dangerouslySetInnerHTML={{__html: product.description}}/>*/}\r\n            <ProductDescription>\r\n              <ReadMore character={600} more={\"Read More\"} less={\"Show Less\"} text={product.description}/>\r\n            </ProductDescription>\r\n\r\n            <ProductCartWrapper>\r\n              <ProductCartBtn>\r\n                {!cartQuantity ? (\r\n                    <Button\r\n                        className=\"cart-button\"\r\n                        variant=\"secondary\"\r\n                        borderRadius={100}\r\n                        onClick={handleAddClick}\r\n                    >\r\n                      <CartIcon mr={2} />\r\n                      <ButtonText>\r\n                        Cart\r\n                      </ButtonText>\r\n                    </Button>\r\n                ) : (\r\n                    <Counter\r\n                        value={cartQuantity}\r\n                        onDecrement={handleRemoveClick}\r\n                        onIncrement={handleAddClick}\r\n                    />\r\n                )}\r\n\r\n              </ProductCartBtn>\r\n            </ProductCartWrapper>\r\n\r\n            <ProductMeta>\r\n              <MetaSingle>\r\n                {product.category&&\r\n                    <Link\r\n                        href={`/?category=${product.category}`}\r\n                        key={`link-${product.category}`}\r\n                    >\r\n                      {\r\n                        <a>\r\n                          <MetaItem category={true}>{product.category}</MetaItem>\r\n                        </a>\r\n                      }\r\n                    </Link>}\r\n\r\n                <ArrowNext/>\r\n                {product.subCategory&&\r\n                <Link\r\n                    href={`/?category=${product.subCategory}`}\r\n                    key={`link-${product.subCategory}`}\r\n                >\r\n                  {\r\n                    <a>\r\n                      <MetaItem category={false}>{product.subCategory}</MetaItem>\r\n                    </a>\r\n                  }\r\n                </Link>}\r\n              </MetaSingle>\r\n            </ProductMeta>\r\n          </ProductInfo>\r\n\r\n          <ProductPreview>\r\n            <BackButton>\r\n              <Button\r\n                  title=\"Back\"\r\n                  intlButtonId=\"backBtn\"\r\n                  iconPosition=\"left\"\r\n                  size=\"small\"\r\n                  style={{\r\n                    backgroundColor: '#ffffff',\r\n                    border: '1px solid #f