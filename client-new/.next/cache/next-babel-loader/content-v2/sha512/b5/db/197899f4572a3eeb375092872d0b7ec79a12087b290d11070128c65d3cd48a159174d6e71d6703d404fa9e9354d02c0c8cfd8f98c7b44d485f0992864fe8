Items, cartItemToAdd\n\nvar addItemToCart = function addItemToCart(state, action) {\n  var existingCartItemIndex = state.items.findIndex(function (item) {\n    return item.id === action.payload.id;\n  });\n\n  if (existingCartItemIndex > -1) {\n    var newState = _toConsumableArray(state.items);\n\n    newState[existingCartItemIndex].quantity += action.payload.quantity;\n    return newState;\n  }\n\n  return [].concat(_toConsumableArray(state.items), [action.payload]);\n}; // cartItems, cartItemToRemove\n\n\nvar removeItemFromCart = function removeItemFromCart(state, action) {\n  return state.items.reduce(function (acc, item) {\n    if (item.id === action.payload.id) {\n      var newQuantity = item.quantity - action.payload.quantity;\n      return newQuantity > 0 ? [].concat(_toConsumableArray(acc), [_objectSpread(_objectSpread({}, item), {}, {\n        quantity: newQuantity\n      })]) : _toConsumableArray(acc);\n    }\n\n    return [].concat(_toConsumableArray(acc), [item]);\n  }, []);\n};\n\nvar clearItemFromCart = function clearItemFromCart(state, action) {\n  return state.items.filter(function (item) {\n    return item.id !== action.payload.id;\n  });\n};\n\nexport var reducer = function reducer(state, action) {\n  switch (action.type) {\n    case 'REHYDRATE':\n      return _objectSpread(_objectSpread({}, state), action.payload);\n\n    case 'TOGGLE_CART':\n      return _objectSpread(_objectSpread({}, state), {}, {\n        isOpen: !state.isOpen\n      });\n\n    case 'ADD_ITEM':\n      return _objectSpread(_objectSpread({}, state), {}, {\n        items: addItemToCart(state, action)\n      });\n\n    case 'REMOVE_ITEM':\n      return _objectSpread(_objectSpread({}, state), {}, {\n        items: removeItemFromCart(state, action)\n      });\n\n    case 'CLEAR_ITEM_FROM_CART':\n      return _objectSpread(_objectSpread({}, state), {}, {\n        items: clearItemFromCart(state, action)\n      });\n\n    case 'CLEAR_CART':\n      return _objectSpread(_objectSpread({}, state), {}, {\n        items: []\n      });\n\n    case 'APPLY_COUPON':\n      return _objectSpread(_objectSpread({}, state), {}, {\n        coupon: action.payload\n      });\n\n    case 'REMOVE_COUPON':\n      return _objectSpread(_objectSpread({}, state), {}, {\n        coupon: null\n      });\n\n    case 'TOGGLE_RESTAURANT':\n      return _objectSpread(_objectSpread({}, state), {}, {\n        isRestaurant: !state.isRestaurant\n      });\n\n    default:\n      throw new Error(\"Unknown action: \".concat(action.type));\n  }\n};","map":{"version":3,"sources":["/home/hari/WebstormProjects/ecommerce-MERN/client-new/src/contexts/cart/cart.reducer.tsx"],"names":["cartItemsTotalPrice","items","coupon","length","itemCost","reduce","total","item","salePrice","quantity","price","discount","Number","discountInPercent","addItemToCart","state","action","existingCartItemIndex","findIndex","id","payload","newState","removeItemFromCart","acc","newQuantity","clearItemFromCart","filter","reducer","type","isOpen","isRestaurant","Error"],"mappings":";;;;;;;AAAA;AACA,OAAO,IAAMA,mBAAmB,GAAG,SAAtBA,mBAAsB,CAACC,KAAD,EAA0B;AAAA,MAAlBC,MAAkB,uEAAT,IAAS;AAC3D,MAAID,KAAK,KAAK,IAAV,IAAkBA,KAAK,CAACE,MAAN,KAAiB,CAAvC,EAA0C,OAAO,CAAP;AAC1C,MAAMC,QAAQ,GAAGH,KAAK,CAACI,MAAN,CAAa,UAACC,KAAD,EAAQC,IAAR,EAAiB;AAC7C,QAAIA,IAAI,CAACC,SAAT,EAAoB;AAClB,aAAOF,KAAK,GAAGC,IAAI,CAACC,SAAL,GAAiBD,IAAI,CAACE,QAArC;AACD;;AACD,WAAOH,KAAK,GAAGC,IAAI,CAACG,KAAL,GAAaH,IAAI,CAACE,QAAjC;AACD,GALgB,EAKd,CALc,CAAjB,CAF2D,CAQ3D;;AACA,MAAME,QAAQ,GAAGT,MAAM,GAClBE,QAAQ,GAAGQ,MAAM,CAACV,MAAM,CAACW,iBAAR,CAAlB,GAAgD,GAD7B,GAEnB,CAFJ,CAT2D,CAY3D;AACA;;AACA,SAAOT,QAAQ,GAAGO,QAAlB;AACD,CAfM,C,CAgBP;;AACA,IAAMG,aAAa,GAAG,SAAhBA,aAAgB,CAACC,KAAD,EAAQC,MAAR,EAAmB;AACvC,MAAMC,qBAAqB,GAAGF,KAAK,CAACd,KAAN,CAAYiB,SAAZ,CAC5B,UAACX,IAAD;AAAA,WAAUA,IAAI,CAACY,EAAL,KAAYH,MAAM,CAACI,OAAP,CAAeD,EAArC;AAAA,GAD4B,CAA9B;;AAIA,MAAIF,qBAAqB,GAAG,CAAC,CAA7B,EAAgC;AAC9B,QAAMI,QAAQ,sBAAON,KAAK,CAACd,KAAb,CAAd;;AACAoB,IAAAA,QAAQ,CAACJ,qBAAD,CA