l };\n    case 'TOGGLE_RESTAURANT':\n      return { ...state, isRestaurant: !state.isRestaurant };\n    default:\n      throw new Error(`Unknown action: ${action.type}`);\n  }\n};\n"]},"metadata":{},"sourceType":"module"}