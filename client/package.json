{
  "name": "typescript-nextjs-starter",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "next export",
    "start": "next start -H 0.0.0.0 -p ${PORT:-8080}",
    "heroku-postbuild": "npm run build",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@next/bundle-analyzer": "^11.0.1",
    "@stripe/react-stripe-js": "^1.4.1",
    "@stripe/stripe-js": "^1.15.1",
    "@styled-system/css": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "@types/react-stickynode": "^3.0.1",
    "@types/styled-components": "^5.1.7",
    "@types/styled-system": "^5.1.10",
    "@types/styled-system__theme-get": "^5.0.1",
    "axios": "^0.21.1",
    "axios-hooks": "^2.4.1",
    "dateformat": "^4.5.1",
    "light-toast": "^0.3.4",
    "next": "^11.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-optimized-images": "^2.6.2",
    "npm": "^7.20.6",
    "rc-collapse": "^3.1.1",
    "rc-drawer": "^4.4.0",
    "rc-table": "^7.17.0",
    "react": "17.0.1",
    "react-content-loader": "^6.0.1",
    "react-custom-scrollbars-2": "^4.4.0",
    "react-dom": "17.0.1",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.2.2",
    "react-multi-carousel": "^2.5.5",
    "react-multi-form": "^1.0.5",
    "react-rating-stars-component": "^2.2.0",
    "react-responsive-modal": "^6.1.0",
    "react-spring": "^8.0.27",
    "react-stickynode": "^3.0.4",
    "react-styled-flexboxgrid": "^3.2.1",
    "react-tabs": "^3.2.2",
    "react-text-mask": "^5.4.3",
    "resize-observer-polyfill": "^1.5.1",
    "styled-components": "^5.3.0",
    "styled-system": "^5.1.5",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "webpack": "^5.21.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "11.0.0",
    "@types/classnames": "^2.2.11",
    "@types/dateformat": "^3.0.1",
    "@types/node": "14.14.25",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@types/react-inner-image-zoom": "^2.0.1",
    "@types/react-tabs": "^2.3.3",
    "@types/styled-system__css": "^5.0.14",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.19.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.8",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
