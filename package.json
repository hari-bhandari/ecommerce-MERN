{
  "name": "fiverr",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon server-new/server",
    "client": "npm run dev --prefix client-new",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node server-new/seeder",
    "data:destroy": "node server-new/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "Hari Bhandari",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.799.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.25.0",
    "colors": "^1.4.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "datauri": "^3.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "i": "^0.3.6",
    "jsonwebtoken": "^8.5.1",
    "mailchimp-api-v3": "^1.15.0",
    "mailgun-js": "^0.22.0",
    "mongoose": "^5.10.6",
    "mongoose-slug-generator": "^1.0.4",
    "mongoose-slug-updater": "^3.3.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "npm": "^7.7.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "react-hooks-forms": "^0.8.1"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
