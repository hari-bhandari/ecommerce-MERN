{
  "name": "Wisecart",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "main": "server.js",
  "scripts": {
    "start": "node server/server",
    "server": "nodemon server/server",
    "admin": "npm start --prefix admin",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run admin\"",
    "dev:admin": "concurrently \"npm run server\" \"npm run admin\"",
    "dev:client": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node server/seeder",
    "data:destroy": "node server/seeder -d"
  },
  "author": "Hari Bhandari",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^3.0.0",
    "cloudinary": "^1.25.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "datauri": "^3.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mailchimp-api-v3": "^1.15.0",
    "mailgun-js": "^0.22.0",
    "mongoose": "^5.12.3",
    "mongoose-simple-random": "^0.4.1",
    "mongoose-slug-generator": "^1.0.4",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.0",
    "stripe": "^8.157.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
